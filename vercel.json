{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "ALLOWALL"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors https://web.telegram.org https://telegram.org https://*.telegram.org"
          }
        ]
      }
    ]
  }
  